# Manifest for SecureApp package

include README.md
include LICENSE
include CHANGELOG.md
include requirements.txt
include pyproject.toml

recursive-include app *.py
recursive-include config *.py
recursive-include tests *.py

recursive-exclude * __pycache__
recursive-exclude * *.py[co]
recursive-exclude * *.so
recursive-exclude * .DS_Store
recursive-exclude * .git*
recursive-exclude * *.log
recursive-exclude * *.db
recursive-exclude * temp/*
recursive-exclude * logs/*
recursive-exclude * data/encrypted/*
recursive-exclude * data/database/*.db
